{
  "name": "wyn",
  "displayName": "Wyn Language Support",
  "description": "Syntax highlighting and LSP support for the Wyn programming language",
  "version": "1.6.0",
  "publisher": "wyn-lang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "wyn",
        "aliases": [
          "Wyn",
          "wyn"
        ],
        "extensions": [
          ".wyn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wyn",
        "scopeName": "source.wyn",
        "path": "./syntaxes/wyn.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "Wyn",
      "properties": {
        "wyn.lsp.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable Wyn Language Server"
        },
        "wyn.lsp.path": {
          "type": "string",
          "default": "wyn",
          "description": "Path to wyn executable"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wynlang/vscode-wyn"
  },
  "homepage": "https://wynlang.com"
}